{
    "name": "rkw/rkw-search",
    "type": "typo3-cms-extension",
    "description": "Extension for a semantic search based on OrientDb",
    "homepage": "https://www.rkw-kompetenzzentrum.de",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "RKW", "Semantic Search", "OrientDb"],
    "support": {
        "email": "developer@steffenkroggel.de"
    },
    "authors": [
        {
            "name": "Steffen Kroggel",
            "email": "developer@steffenkroggel.de",
            "homepage": "http://www.steffenkroggel.de",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^7.6.0 || ^8.7.0",
        "doctrine/orientdb-odm": "dev-master@dev",
        "orientdb-php/orientdb-php": "0.4.9",
        "rkw/rkw-basics": "~7.6.12",
        "rkw/rkw-authors": "~7.6.10",
        "rkw/rkw-projects": "~7.6.10",
        "rkw/rkw-geolocation": "~7.6.10"
    },
    "autoload": {
        "psr-4": {
            "RKW\\Search\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "rkw_search"
        }
    }
}